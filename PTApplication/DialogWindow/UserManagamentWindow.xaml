<Window x:Class="PTApplication.DialogWindow.UserManagamentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PTApplication.DialogWindow"
        mc:Ignorable="d"
        Title="UserManagamentWindow" Height="450" Width="800">
    <Grid>
        <DataGrid Name="DG1" ItemsSource="{Binding}" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" />
                <DataGridTextColumn Header="Login"></DataGridTextColumn>
                <DataGridCheckBoxColumn Header="Is loged in"></DataGridCheckBoxColumn>
                <DataGridTextColumn Header="Password"></DataGridTextColumn>
                <DataGridTextColumn Header="Ip"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
